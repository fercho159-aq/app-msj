{
  "expo": {
    "name": "Yaakob",
    "slug": "consultora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.fernandotrejo.consultora",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "Este app necesita acceso al micrófono para realizar llamadas de voz",
        "NSCameraUsageDescription": "Este app necesita acceso a la cámara para realizar videollamadas",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "UIBackgroundModes": [
          "audio",
          "voip",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fernandotrejo.consultora"
    },
    "plugins": [
      "./plugins/withAndroidCleartextTraffic",
      "./plugins/withWebRTCPermissions",
      "expo-font",
      [
        "expo-notifications",
        {
          "sounds": []
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "Yaakob",
      "shortName": "Yaakob",
      "description": "Be Heart - Conecta con tu corazón.",
      "themeColor": "#000000",
      "backgroundColor": "#ffffff",
      "display": "standalone"
    },
    "extra": {
      "eas": {
        "projectId": "efc8aa09-d475-421c-a86c-a5311777e3dc"
      },
      "EXPO_PUBLIC_API_URL": "https://appsoluciones.duckdns.org/api",
      "EXPO_PUBLIC_SPREED_WS_URL": "wss://calls.mawsoluciones.com/ws"
    },
    "owner": "maestrosdelmediamdm"
  }
}
